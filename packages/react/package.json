{
	"private": false,
	"name": "@formulier/react",
	"version": "0.2.7",
	"license": "MIT",
	"author": "Jasper Van Gestel <vjee2e@gmail.com> (https://github.com/vjee)",
	"description": "Simple, performant form library for React",
	"homepage": "https://vjee.github.io/formulier/",
	"keywords": [
		"formulier",
		"@formulier/core",
		"@formulier/react",
		"form",
		"forms",
		"controlled",
		"conditional",
		"validation",
		"react",
		"react-dom",
		"use-sync-external-store",
		"hook",
		"hooks"
	],
	"type": "module",
	"main": "./src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public",
		"main": "./dist/index.js",
		"exports": {
			".": "./dist/index.js",
			"./package.json": "./package.json"
		},
		"types": "./dist/index.d.ts"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsup --watch",
		"lint": "eslint src tests cypress --ext .ts,.tsx --fix",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"e2e": "cypress run --component"
	},
	"dependencies": {
		"@formulier/core": "workspace:*",
		"use-sync-external-store": "^1.2.2"
	},
	"devDependencies": {
		"@formulier/core": "workspace:0.2.6",
		"@formulier/eslint-config": "workspace:*",
		"@formulier/tsconfig": "workspace:*",
		"@testing-library/react": "^16.0.1",
		"@types/react": "^18.3.10",
		"@types/react-dom": "^18.3.0",
		"@types/shallow-clone": "^3.0.2",
		"@types/use-sync-external-store": "^0.0.6",
		"@vitest/coverage-istanbul": "^2.1.1",
		"cypress": "^13.15.0",
		"eslint": "^8.57.1",
		"jsdom": "^25.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"vite": "^5.4.8",
		"vitest": "^2.1.1"
	},
	"peerDependencies": {
		"react": ">=16.8.0"
	}
}
