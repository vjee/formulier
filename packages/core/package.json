{
	"private": false,
	"name": "@formulier/core",
	"version": "0.0.2",
	"license": "MIT",
	"author": "Jasper Van Gestel <vjee2e@gmail.com> (https://github.com/vjee)",
	"type": "module",
	"exports": {
		".": {
			"import": "./src/index.ts"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public",
		"exports": {
			".": {
				"import": "./dist/index.js",
				"require": "./dist/index.cjs",
				"default": "./dist/index.js"
			},
			"./package.json": "./package.json"
		},
		"types": "./dist/index.d.ts"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"lint": "eslint src --ext .ts,.tsx",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"fast-deep-equal": "^3.1.3",
		"shallow-clone": "^3.0.1"
	},
	"devDependencies": {
		"@formulier/eslint-config": "workspace:0.0.0",
		"@formulier/tsconfig": "workspace:0.0.0",
		"eslint": "^7.23.0",
		"react": "^18.2.0",
		"tsup": "^6.2.3"
	},
	"eslintConfig": {
		"root": true,
		"extends": [
			"@formulier/eslint-config"
		]
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"format": [
			"esm",
			"cjs"
		],
		"dts": true,
		"minify": true,
		"clean": true
	},
	"packageManager": "pnpm@7.5.2"
}
