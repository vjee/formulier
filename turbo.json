{
	"$schema": "https://turborepo.org/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"dev": {
			"cache": false
		},
		"typecheck": {
			"outputs": []
		},
		"test": {
			"outputs": []
		},
		"coverage": {
			"outputs": []
		},
		"e2e": {
			"outputs": []
		}
	}
}
